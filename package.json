{
  "name": "@genexus/web-controls-library-react",
  "sideEffects": false,
  "version": "0.32.1",
  "private": false,
  "description": "React specific wrapper for @genexus/web-controls-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/genexuslabs/web-controls-library-react.git"
  },
  "scripts": {
